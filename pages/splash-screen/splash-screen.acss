.splash-screen-container{
  width: 100%;
  min-height: 100vh;
  display:flex;
  justify-content: center;
  align-items: center;
  z-index: 1;
}
.image-card{
  width: 250px;
  height: 250px;
}
.splash-logo{
  width: 250px;
  height: 250px;
  z-index: 10;
  object-fit: contain;
  animation: popup-animation 1s ease-in-out;
}

@keyframes popup-animation{
  0% {
    transform: scale(0);
    opacity: 0;
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}